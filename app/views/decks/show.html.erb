<% if notice.present? %>
  <p class="flash mt-2"><%= notice %></p>
<% end %>

<div class="my-2">
  <%= link_to "Edit", edit_deck_path(@deck), class: "btn mr-2" %>
  <%= link_to "Back", decks_path, class: "btn-link" %>
</div>
<div class="Box my-2">
  <div class="Box-header">
    <h3 class="Box-title">
      <%= @deck.title %>
    </h3>
  </div>
  <div class="Box-body">
    <h4>Raw content</h4>
    <pre><%= @deck.raw %></pre>
  </div>
</div>
